{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[{
      "name":"Technology-api",
      "urls":["http://www.json-generator.com/api/json"],
      "cacheConfig" :{
        "strategy": "freshness",
        "maxSize" : 20,
        "maxAge" : "1h",
        "timeout":"5s"
      }
    }]
}
